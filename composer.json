{
    "name": "rohsyl/laravel-advanced-query-filter",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "rohsyl",
            "email": "syzin12@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3|^7.4|^8.0",
        "illuminate/container": "^6.0|^7.0|^8.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "maatwebsite/excel": "^3.1",
        "carlos-meneses/laravel-mpdf": "^2.1"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "rohsyl\\LaravelAdvancedQueryFilter\\": "src/"
        },
        "files": [
            "helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "rohsyl\\LaravelAdvancedQueryFilter\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "rohsyl\\LaravelAdvancedQueryFilter\\ServiceProvider"
            ],
            "aliases":
            {
                "QueryFilterUrl": "rohsyl\\LaravelAdvancedQueryFilter\\QueryFilterUrl",
                "Filters": "rohsyl\\LaravelAdvancedQueryFilter\\Filters"
            }
        }
    }
}
